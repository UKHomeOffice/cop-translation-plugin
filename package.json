{
  "name": "translation-service",
  "version": "0.0.3",
  "private": true,
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "test": "node_modules/.bin/mocha --require babel-polyfill --require babel-register --exit --recursive --timeout 5000",
    "coverage": "nyc --reporter=lcov mocha --require babel-polyfill --require babel-register --exit --recursive --timeout 5000"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "app-root-path": "^2.1.0",
    "async": "^2.6.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "eslint-plugin-import": "^2.10.0",
    "express": "^4.16.2",
    "express-http-context": "^1.2.1",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "formiojs": "^2.30.2",
    "helmet": "^3.12.0",
    "jsonpath": "^1.0.1",
    "keycloak-connect": "^4.5.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.0",
    "morgan": "^1.9.0",
    "nsp": "^3.2.1",
    "requirejs": "^2.3.5",
    "static-eval": "^2.0.2",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.9.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.0.0",
    "eslint": "^4.19.1",
    "expect": "^22.4.3",
    "mocha": "^5.0.5",
    "nock": "^9.2.3",
    "node-mocks-http": "^1.5.8",
    "nodemon": "^1.12.1",
    "nyc": "^13.3.0",
    "sinon": "^4.5.0"
  }
}
