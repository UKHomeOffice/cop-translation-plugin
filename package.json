{
  "name": "translation-service",
  "version": "0.0.3",
  "private": true,
  "main": "dist",
  "scripts": {
    "build": "babel src -s -D -d dist --presets es2015,stage-0 && webpack",
    "test": "node_modules/.bin/mocha --require babel-polyfill --require babel-register --exit --recursive --timeout 5000",
    "coverage": "nyc --reporter=lcov mocha --require babel-polyfill --require babel-register --exit --recursive --timeout 5000",
    "linter": "eslint src test"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "es6": true,
      "mocha": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "app-root-path": "^2.2.1",
    "async": "^2.6.3",
    "axios": "^0.18.1",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "body-parser": "^1.19.0",
    "cls-hooked": "^4.2.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "eslint-plugin-import": "^2.18.2",
    "express": "^4.17.1",
    "express-http-context": "^1.2.3",
    "express-session": "^1.17.0",
    "express-validator": "^4.3.0",
    "formiojs": "^2.30.2",
    "helmet": "^3.21.2",
    "https-proxy-agent": "^3.0.1",
    "ioredis": "^4.14.1",
    "jsonpath": "^1.0.2",
    "keycloak-connect": "^4.5.0",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "morgan": "^1.9.0",
    "requirejs": "^2.3.5",
    "static-eval": "^2.0.2",
    "uuid": "^3.3.3",
    "validator": "^12.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.9.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "eslint": "^4.19.1",
    "expect": "^25.0.0",
    "ioredis-mock": "^4.18.2",
    "mocha": "^5.0.5",
    "mocha-param": "^2.0.0",
    "nock": "^9.2.3",
    "node-mocks-http": "^1.8.0",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "sinon": "^7.5.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
